{
  "name": "goodroute-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "node dist/src/main.js",
    "start:dev": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm-run-all -p copy:*",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:rollback": "npm run typeorm migration:revert",
    "copy:email-templates": "copyfiles -u 1 src/**/*.hbs dist/src",
    "copy:json": "copyfiles -u 1 src/**/*.json dist/src",
    "predeploy:beta": "npm run build && cd client && npm run build",
    "deploy:beta": "gcloud app deploy --project=goodroute-beta",
    "graphql:ts": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.5.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.0",
    "@nestjs/graphql": "^7.9.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.5.0",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "date-fns": "^2.16.1",
    "express-enforces-ssl": "^1.1.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.1.0",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.6",
    "helmet": "^4.1.0",
    "nestjs-twilio": "^1.0.2",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "twilio": "^3.55.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@nestjs/cli": "^7.5.0",
    "@nestjs/schematics": "^7.2.0",
    "@nestjs/testing": "^7.5.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "copyfiles": "^2.4.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^2.1.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2",
    "npm-run-all": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "node": "12.20.1",
    "yarn": "1.22.10"
  }
}
